syntax on
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set number
set hlsearch
set ruler
set t_Co=256
highlight Braces ctermfg=white    
highlight Comment ctermfg=238 cterm=none    
highlight Constant ctermfg=white 
highlight Define ctermfg=white 
highlight Function ctermfg=white cterm=none    
highlight Identifier ctermfg=white cterm=none    
highlight Include ctermfg=white cterm=none
highlight Operator ctermfg=white cterm=none
highlight PreProc ctermfg=white cterm=none    
highlight Special ctermfg=white cterm=none    
highlight Statement ctermfg=white cterm=none 
highlight String ctermfg=white
highlight Todo ctermfg=black ctermbg=white cterm=bold    
highlight Type ctermfg=white cterm=none    
highlight LineNr ctermfg=238
highlight OverLength ctermbg=darkred ctermfg=white guibg=#FFD9D9
match OverLength /\%>80v.\+/

